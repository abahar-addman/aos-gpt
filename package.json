{
	"name": "open-webui",
	"version": "0.8.3",
	"private": true,
	"scripts": {
		"dev": "concurrently \"bun run dev:backend\" \"bun run dev:frontend\"",
		"dev:frontend": "bun run pyodide:fetch && vite dev --host",
		"dev:backend": "cd backend && venv/Scripts/python -m uvicorn open_webui.main:app --reload --host 0.0.0.0 --port 8080",
		"dev:5050": "bun run pyodide:fetch && vite dev --port 5050",
		"build": "bun run pyodide:fetch && vite build",
		"build:watch": "bun run pyodide:fetch && vite build --watch",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "bun run lint:frontend && bun run lint:types && bun run lint:backend",
		"lint:frontend": "eslint . --fix",
		"lint:types": "bun run check",
		"lint:backend": "pylint backend/",
		"format": "prettier --plugin-search-dir --write \"**/*.{js,ts,svelte,css,md,html,json}\"",
		"format:backend": "black . --exclude \".venv/|/venv/\"",
		"i18n:parse": "i18next --config i18next-parser.config.ts && prettier --write \"src/lib/i18n/**/*.{js,json}\"",
		"cy:open": "cypress open",
		"test:frontend": "vitest --passWithNoTests",
		"pyodide:fetch": "bun scripts/prepare-pyodide.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "3.2.2",
